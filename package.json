{
    "author": "Scotley <scott@inspireweb.co.nz>",
    "description": "Simple Typescript decorators to improve the vuex-orm experience in Typescript by introducing typed property access.",
    "devDependencies": {
        "@vuex-orm/core": "^0.32.1",
        "tslib": "^1.10.0",
        "typescript": "^3.6.2",
        "vue": "^2.6.10",
        "vuex": "^3.1.1"
    },
    "es2015": "dist/es2015/index.js",
    "esnext": "dist/esnext/index.js",
    "files": [
        "dist"
    ],
    "keywords": [
        "vuex-orm",
        "typescript",
        "decorators",
        "es7"
    ],
    "license": "MIT",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "name": "vuex-orm-decorators",
    "peerDependencies": {
        "@vuex-orm/core": "*",
        "tslib": "*",
        "typescript": "*",
        "vue": "*",
        "vuex": "*"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/scotley/vuex-orm-decorators.git"
    },
    "scripts": {
        "build": "npm run build:esnext && npm run build:es2015 && npm run build:esm && npm run build:cjs",
        "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
        "build:es2015": "tsc --module es2015 --target es2015 --outDir dist/es2015",
        "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
        "build:esnext": "tsc --module esnext --target esnext --outDir dist/esnext",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "types": "dist/types/index.d.ts",
    "typings": "dist/types/index.d.ts",
    "version": "1.1.7"
}
